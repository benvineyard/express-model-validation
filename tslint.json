{
	"rules": {
		"no-unused-expression": true,
		"no-duplicate-variable": true,
		"no-duplicate-key": true,
		"no-unused-variable": [
			true,
			{
				"ignore-pattern": "^(^validation|^should)"
			}
		],
		"curly": true,
		"class-name": true,
		"semicolon": [
			"always"
		],
		"triple-equals": true,
		"no-eval": true,
		"use-strict": [
			true
		],
		"no-any": true,
		"no-consecutive-blank-lines": true,
		"no-empty": true,
		"no-var-keyword": true,
		"no-var-requires": true,
		"member-access": [
			"check-accessor"
		],
		"member-ordering": [
			"private",
			"variables-before-functions"
		],
		"quotemark": [
			true,
			"single",
			"avoid-escape"
		],
		"indent": [
			true,
			"tabs"
		],
		"typedef": [
			true,
			"call-signature",
			"property-declaration",
			"member-variable-declaration"
		],
		"typedef-whitespace": [
			true,
			{
			"call-signature": "nospace",
			"index-signature": "nospace",
			"parameter": "nospace",
			"property-declaration": "nospace",
			"variable-declaration": "nospace"
			}
		]
	}
}